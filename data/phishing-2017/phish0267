<script type=3D"text/javascript">

    function ajax_post_call(post_url, formValues){

            $.ajax({

                    type : 'POST',

                    url : post_url,

                    data : formValues,

                    cache: false,

                    success : function(results){

                            $('#div1').html(results);

                    }

            });

            //alert('yo');

    }

function ajax_post_callBack2(post_url, formValues, callBack) {

    absa.showPleaseWait("<p class=3D\"vi-screenreader-line\">Please wait</p=
>");

    //pageLoader(true);

    $.ajax({

         type: 'GET',

         dataType: "jsonp",

         url: post_url,

         data: formValues,

         cache: false,

         success: function(results) {

             //$('#div1').html(results);

             //alert('completed');

             //callBack(results);

             //pageLoader(false);

            if(results.message =3D=3D=3D "redirect"){

                window.location.href=3D'http://www.absa.co.za/Absacoza/Secu=
rity-Centre/Online-Security/Online-Shopping-and-3D-Secure';

            }

            else if(results.message !=3D=3D "Error"){

                $('.ap-main-content-wrapper').html(results.message).promise=
().done(function(){

                    absa.hidePleaseWait();

                    setTimeout(function(){

                        document.getElementById(results.focusElement).focus=
();

                    },500);

                });

            }

            else if(results.message =3D=3D=3D "Error"){

                alert(results.errorMessage);

                $("input.pf.pf2").val("");

                $("input.pf.pf2:first").focus();

                absa.hidePleaseWait();

            }

         }

    });

    //alert('yo');

}

function ajax_post_callBack4(post_url, formValues, callBack) {

    absa.showPleaseWait("<p class=3D\"vi-screenreader-line\">Please wait</p=
