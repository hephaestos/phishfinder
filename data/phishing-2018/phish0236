    return stringInput.replace(regex, '');
};

jsCommon.IsDayOfWeekValid =3D function(startDate, endDate, intDateDifferenc=
e, stringDayOfWeek)
{
    var weekday=3Dnew Array(7);
    weekday[0]=3D"Sunday";
    weekday[1]=3D"Monday";
    weekday[2]=3D"Tuesday";
    weekday[3]=3D"Wednesday";
    weekday[4]=3D"Thursday";
    weekday[5]=3D"Friday";
    weekday[6]=3D"Saturday";
     =

    if((weekday[startDate.getDay()].toLowerCase() =3D=3D=3D stringDayOfWeek=
.toLowerCase()) ||
        (weekday[endDate.getDay()].toLowerCase() =3D=3D=3D stringDayOfWeek.=
toLowerCase())){
        return true;
        } =

    =

    for(var i=3D0;i<intDateDifference;i++){
        var nextDate =3D new Date(startDate.setDate(startDate.getDate() + 1=
));        =

        if(weekday[nextDate.getDay()].toLowerCase() =3D=3D=3D stringDayOfWe=
ek.toLowerCase()){            =

            return true;
        }
    }    =

    return false;
};

jsCommon.getFormElement =3D function (controlPrefix, inputType, controlId) {

    var isTableInput =3D false;

    if (controlId.startsWith("tbl") || controlId.startsWith("txt"))
        isTableInput =3D true;

    var jquerySelect =3D String.format('{0}[id$=3D"_{1}"]', inputType, cont=
rolId);

    if (isTableInput)
        jquerySelect =3D String.format('{0}[id$=3D"{1}"]', inputType, contr=
olId);

    var formControl =3D $(jquerySelect);

    if (formControl =3D=3D null || formControl =3D=3D undefined) {
        if (isTableInput) {
            formControl =3D document.getElementById(controlId);
        } else {
            formControl =3D document.getElementById(String.format("{0}_{1}"=
, controlPrefix, controlId));
        }

    }

    if (formControl =3D=3D null || formControl =3D=3D undefined)
        alert("Could not find " + controlId);

    return formControl;
};

jsCommon.getFormElementByName =3D function (controlPrefix, inputType, contr=
olId) {

    var jquerySelect =3D String.format('{0}[name$=3D"${1}"]', inputType, co=
ntrolName);

    var formControl =3D $(jquerySelect);

    if (formControl =3D=3D null || formControl =3D=3D "undefined")
        formControl =3D document.getElementById(String.format("{0}${1}", co=
ntrolPrefix, controlName));

    if (formControl =3D=3D null || formControl =3D=3D "undefined")
        alert("Could not find " + controlName);

    return formControl;
};

jsCommon.getRadioButtonValue =3D function (controlName) {
    var jquerySelect =3D String.format('input:radio[name$=3D"${0}"]:checked=
', controlName);

    var formControl =3D $(jquerySelect);
    return formControl
};

String.format =3D function () {
    var s =3D arguments[0];
    for (var i =3D 0; i < arguments.length - 1; i++) {
        var reg =3D new RegExp("\\{" + i + "\\}", "gm");
        s =3D s.replace(reg, arguments[i + 1]);
    }
    return s;
};

String.prototype.endsWith =3D function (suffix) {
    return (this.substr(this.length - suffix.length) =3D=3D=3D suffix);
};
String.prototype.startsWith =3D function (prefix) {
    return (this.substr(0, prefix.length) =3D=3D=3D prefix);
};


//The date popup is used across the entire site, seems to make sense to hav=
e it in
//the common file
jsCommon.DateSelectorPopup =3D function (anchorElement, inputId) {

    var inputPurchaseDate =3D document.getElementById(inputId);

    MyCalendar =3D new calendar1(inputPurchaseDate);
    MyCalendar.year_scroll =3D true;
    MyCalendar.time_comp =3D false;
    MyCalendar.OriginatorButtonPosition =3D jsCalendar.FindPosition(anchorE=
lement);
    if (inputPurchaseDate.value !=3D "") {
        MyCalendar.popup();
    }
    else {
        MyCalendar.popup(jsCommon.GetNextDateString());
    }

};

//Helper method for the calendar popup
jsCommon.GetNextDateString =3D function () {
    var startPaymentDate =3D new Date();
    startPaymentDate.addDays(1);

    var sMonth =3D new String(Number(startPaymentDate.getMonth() + 1));
    var sDay =3D new String(startPaymentDate.getDate());
    return ((sDay.length < 2) ? "0" + sDay : sDay) + "/" + ((sMonth.length =
< 2) ? "0" + sMonth : sMonth) + "/" + startPaymentDate.getFullYear();
};

