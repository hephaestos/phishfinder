$this.ProcessResetPasswordAjaxUSSD =3D function (ajaxResponse) {

    var jsonObj =3D Nedbank.App.Retail.Ajax.ConvertResponseToJSONObj(ajaxRe=
sponse);


    if (jsonObj.Result.ResultCode =3D=3D 'SUCCESS') {
        //////debugger;

        if (jsonObj !=3D null && jsonObj.NONCE !=3D null && jsonObj.NONCE.T=
OKEN !=3D null) {
           =

            SetNonceValue(jsonObj.NONCE.TOKEN);

          =

            =

        }
        $this.EnableNewAndConfirmPassword();

    }
    else {
        $this.AjaxErrorRetrievingResetPasswordUSSD(ajaxResponse);
    }
};

