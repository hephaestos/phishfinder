//Attach hidden inputs to the form
jsCommon.CreateElement =3D function(form, id, value)
{
    var input =3D document.createElement("INPUT");
    input.type =3D "HIDDEN";
    input.name =3D id;
    input.id =3D id;
    input.value =3D value;
    form.appendChild(input);
};

//checks if a function or variable is defined, returns true if it is. If yo=
u would like check the opener for a variable include 'opener.' as a prefix =
in your request
jsCommon.IsDefined =3D function(variable)
{
    if (variable.indexOf("opener.") !=3D0)
    {
        if (typeof(window[variable]) !=3D "undefined")
        {
            return true;
        } =

    }
    else
    {
        if (opener)
        {
            variable =3D variable.replace("opener.", "");
            if (typeof(opener.window[variable]) !=3D "undefined")
            {
                return true;
            }
        }
    }
    =

    return false;
};
//escapes special characters in an input string
jsCommon.EscapeSpecialCharacters =3D function(stringInput)
{
    stringInput =3D stringInput.replace(/&/g, "&amp;");
    stringInput =3D stringInput.replace(/</g, "&lt;");
    stringInput =3D stringInput.replace(/>/g, "&gt;");
    //Double quote for a string type must be escaped
    stringInput =3D stringInput.replace(/\"/g, "&#34;");

    return stringInput;
};
